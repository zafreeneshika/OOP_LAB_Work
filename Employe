public class Employe {
    int id;
    String name;
    Employe(int id,String name){
        this.id=id;
        this.name=name;
    }
    void info(){
        System.out.println(id);
        System.out.println(name);
    }

    public static void main(String[] args) {
        Employe ob1=new Employe(4701, "Zafreen");

        ob1.info();

    }
